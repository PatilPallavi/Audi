<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Properties"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <heading
            jcr:primaryType="cq:Widget"
            fieldLabel="Heading Text:"
            name="./heading"
            xtype="textfield"/>
        <searchDealertext
            jcr:primaryType="cq:Widget"
            fieldLabel="Search Dealer Text:"
            name="./searchDealertext"
            xtype="textarea"/>
        <searchText
            jcr:primaryType="cq:Widget"
            fieldLabel="Search Text:"
            name="./searchText"
            xtype="textfield"/>
        <errorMessage
            jcr:primaryType="cq:Widget"
            fieldLabel="Error Message:"
            name="./errorMessage"
            xtype="textfield"/>
    </items>
</jcr:root>
